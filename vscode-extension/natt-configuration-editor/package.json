{
  "name": "natt-configuration-editor",
  "displayName": "NATT Configuration Editor",
  "description": "Test scenario configuration editor for black box network application testing tool.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.91.1"
  },
  "categories": [
    "Other"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "extension.nattInit",
        "title": "NATT Init"
      },
      {
        "command": "extension.nattRun",
        "title": "NATT Run"
      },
      {
        "command": "extension.nattStop",
        "title": "NATT Stop"
      },
      {
        "command": "extension.nattValidate",
        "title": "NATT Validate"
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "nattContainer",
          "title": "NATT Configuration Editor",
          "icon": "resources/icon.svg"
        }
      ]
    },
    "views": {
      "nattContainer": [
        {
          "type": "webview",
          "id": "homeView",
          "name": "Home"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.92.0",
    "@types/mocha": "^10.0.7",
    "@types/node": "20.x",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.11.0",
    "eslint": "^8.57.0",
    "typescript": "^5.4.5",
    "@vscode/test-cli": "^0.0.9",
    "@vscode/test-electron": "^2.4.0"
  },
  "files": [
    "out/**/*",
    "resources/**/*"
  ]
}